{
  "name": "ts-mapper",
  "version": "1.1.5",
  "description": "Object mapper for TypeScript",
  "scripts": {
    "build": "tsc",
    "sample": "tsc && node ./lib/sample/main.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "author": "Marluan Espiritusanto",
  "license": "MIT",
  "files": ["lib/**/*"],
  "main": "./lib/typemapper.js",
  "types": "./lib/typemapper.d.ts",
  "directories": {
    "lib": "dist"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "mapper",
    "automapper",
    "typemapper",
    "javascript"
  ],
  "devDependencies": {
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}
